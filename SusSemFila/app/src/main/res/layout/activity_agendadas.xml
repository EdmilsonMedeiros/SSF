<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".AgendadasActivity">

    <ListView
        android:id="@+id/lView"
        android:layout_width="382dp"
        android:layout_height="480dp"
        android:layout_marginTop="128dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="105dp"
        android:layout_height="130dp"
        android:layout_marginEnd="256dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/botaoconsultaagendadas" />

    <TextView
        android:id="@+id/tvNomeUsuario"
        android:layout_width="190dp"
        android:layout_height="25dp"
        android:layout_marginEnd="78dp"
        android:layout_marginTop="36dp"
        android:text="Nome Usuario"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvCpfUsuario"
        android:layout_width="190dp"
        android:layout_marginEnd="78dp"
        android:layout_height="wrap_content"
        android:text="Cpf Usuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNomeUsuario" />
</android.support.constraint.ConstraintLayout>